vents()\r\n    },\r\n    bindEvents :()=>{\r\n        const hashTable ={\r\n        '#btnPause':player.pause,\r\n        '#btnPlay':player.play,\r\n        '#btnSlow':player.slow,\r\n        '#btnNormal':player.normal,\r\n        '#btnFast':player.fast\r\n        }\r\n        for(let key in hashTable){\r\n            document.querySelector(key).onclick = hashTable[key]\r\n        }\r\n    },\r\n    run : () =>{\r\n        n +=1\r\n        if(n >string.length){\r\n            window.clearInterval(id)\r\n            return\r\n        }\r\n        demo.innerHTML = string.substr(0,n)\r\n        demo2.innerText = string.substr(0,n)\r\n        demo2.scrollTop = demo2.scrollHeight\r\n    },\r\n    play: ()=>{\r\n       id = setInterval(player.run,time)\r\n    },\r\n    pause : () =>{\r\n        window.clearInterval(id)\r\n    },\r\n    slow : ()=>{\r\n        player.pause()\r\n        time = 300\r\n        player.play()\r\n    },\r\n    normal : () =>{\r\n        player.pause()\r\n        time = 100\r\n        player.play()\r\n    },\r\n    fast : () =>{\r\n        player.pause()\r\n        time = 0\r\n        player.play()\r\n    }\r\n\r\n}\r\n\r\nplayer.init()\r\n\r\n\r\n\r\n\r\n\r\n\r\n"},"lineCount":null}},"error":null,"hash":"35e4c9575599f07a63f92d0363e8a84a","cacheData":{"env":{}}}